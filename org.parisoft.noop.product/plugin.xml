<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="noopProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            description="NES Object Oriented Programming IDE"
            name="NOOPr">
         <property
               name="appName"
               value="NOOPr">
         </property>
         <property
               name="startupForegroundColor"
               value="BB2024">
         </property>
         <property
               name="startupMessageRect"
               value="7,274,445,16">
         </property>
         <property
               name="startupProgressRect"
               value="5,292,445,8">
         </property>
         <property
               name="aboutImage"
               value="images/About.png">
         </property>
         <property
               name="aboutText"
               value="NOOPr  - NES/Famicom Object Oriented Programming IDE&#x0A;Version: 1.0&#x0A;&#x0A;NOOP is a high-level programming language for the development of 8 bit Nintendo games.&#x0A;NOOPr is the official NOOP IDE. It&apos;s provided with an editor and a compiler for the NOOP language to help transform your game ideas into reality&#x0A;&#x0A;Visit https://github.com/Parisoft/noop">
         </property>
         <property
               name="windowImages"
               value="images/Image16.png,images/Image32.png,images/Image48.png,images/Image64.png,images/Image128.png,images/Image256.png">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>

</plugin>
